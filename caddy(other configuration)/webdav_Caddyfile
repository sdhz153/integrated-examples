#一、网盘应用1
#.........<省略>........
	@notget {
		not method GET
	}
	route @notget {
		basicauth { #可以设置多组用户及密码
			user JDJhJDE0JGMzclpVMkU4bVhMRVRXY0o2a2pmMnVGUDNqcGxGZ3ByY1ZTMEVDbi80N1M0NE93NUZXZXNl #webdav用户与webdav密码（示例为password的哈希密码），修改为自己的。
		}
		webdav {
			root /home/webdav #修改为自己的webdav服务器存放文件路径
		}
	}
#.........<省略>........
#备注：
#1、本配置适合为个人或特定人群提供基本的文件管理服务。仅支持webdav客户端（有验证）浏览文件，可删除、上传、修改等。
#2、在本人所有示例的回落或代理网站配置之前插入上边配置，就可实现webdav服务器与回落或代理网站共存。
#3、修改/home/webdav的webdav权限为0777，以便客户端具有写入及删除文件权限。
#4、basicauth中密码必须使用哈希密码，可用caddy hash-password --plaintext <password>命令把普通密码转化为哈希密码并输出。例如：/usr/bin/caddy hash-password --plaintext password


#二、网盘应用2
#.........<省略>........
	@host {
		host xx.yy #限定域名访问（禁止以ip方式访问网站），修改为自己的域名。
	}
	route @host {
		root * /home/webdav #修改为自己的服务器存放文件路径
		header {
			Strict-Transport-Security "max-age=31536000; includeSubDomains; preload" #启用HSTS
		}
		@notget {
			not method GET
		}
		route @notget {
			basicauth { #可以设置多组用户及密码
				user JDJhJDE0JGMzclpVMkU4bVhMRVRXY0o2a2pmMnVGUDNqcGxGZ3ByY1ZTMEVDbi80N1M0NE93NUZXZXNl #webdav用户与webdav密码（示例为password的哈希密码），修改为自己的。
			}
			webdav
		}
		file_server browse
	}
#.........<省略>........
#备注：
#1、本配置适合为普通人群提供文件分享及专业的文件管理服务。
#1)、支持web登录（无验证）浏览文件，可下载、音视频文件在线播放等。
#2)、支持webdav客户端（有验证）浏览文件，可删除、上传、修改等。
#2、本人所有示例的回落或代理网站都可直接替换为上边配置，静态网站改为文件服务器网站。web登录方法：域名，如“xx.yy”。
#3、修改/home/webdav的webdav权限为0777，以便客户端具有写入及删除文件权限。
#4、basicauth中密码必须使用哈希密码，可用caddy hash-password --plaintext <password>命令把普通密码转化为哈希密码并输出。例如：/usr/bin/caddy hash-password --plaintext password


#三、网盘应用3
#.........<省略>........
	root * /home/webdav #修改为自己的服务器存放文件路径
	handle_path /dav* { #web登录的路径匹配，修改为自己的。
		file_server browse
	}
	@notget {
		not method GET
	}
	route @notget {
		basicauth { #可以设置多组用户及密码
			user JDJhJDE0JGMzclpVMkU4bVhMRVRXY0o2a2pmMnVGUDNqcGxGZ3ByY1ZTMEVDbi80N1M0NE93NUZXZXNl #webdav用户与webdav密码（示例为password的哈希密码），修改为自己的。
		}
		webdav
	}
#.........<省略>........
#备注：
#1、本配置适合为普通人群提供文件分享及专业的文件管理服务。
#1)、支持web登录（无验证）浏览文件，可下载、音视频文件在线播放等。
#2)、支持webdav客户端（有验证）浏览文件，可删除、上传、修改等。
#2、在本人所有示例的回落或代理网站配置之前插入上边配置，就可实现文件服务器网站与回落或代理网站共存。web登录方法：域名/路径，如“xx.yy/dav”。
#3、修改/home/webdav的webdav权限为0777，以便客户端具有写入及删除文件权限。
#4、authentication中密码必须使用哈希密码，可用caddy hash-password --plaintext <password>命令把普通密码转化为哈希密码并输出。例如：/usr/bin/caddy hash-password --plaintext password


#四、网盘应用4
#.........<省略>........
	root * /home/webdav #修改为自己的服务器存放文件路径
	handle_path /dav* { #web登录的路径匹配，修改为自己的。
		basicauth { #可以设置多组用户及密码
			user1 JDJhJDE0JGMzclpVMkU4bVhMRVRXY0o2a2pmMnVGUDNqcGxGZ3ByY1ZTMEVDbi80N1M0NE93NUZXZXNl #web登录用户与web登录密码（示例为password的哈希密码），修改为自己的。
		}
		file_server browse
	}
	@notget {
		not method GET
	}
	route @notget {
		basicauth { #可以设置多组用户及密码
			user2 JDJhJDE0JGMzclpVMkU4bVhMRVRXY0o2a2pmMnVGUDNqcGxGZ3ByY1ZTMEVDbi80N1M0NE93NUZXZXNl #webdav用户与webdav密码（示例为password的哈希密码），修改为自己的。
		}
		webdav
	}
#.........<省略>........
#备注：
#1、本配置适合为特定人群提供文件分享及专业的文件管理服务。
#1)、支持web登录（有验证）浏览文件，可下载、音视频文件在线播放等。
#2)、支持webdav客户端（有验证）浏览文件，可删除、上传、修改等。
#3)、web登录与webdav客户端，两者用户及密码各自独立。
#2、在本人所有示例的回落或代理网站配置之前插入上边配置，就可实现文件服务器网站与回落或代理网站共存。web登录方法：域名/路径，如“xx.yy/dav”。
#3、修改/home/webdav的webdav权限为0777，以便客户端具有写入及删除文件权限。
#4、basicauth中密码必须使用哈希密码，可用caddy hash-password --plaintext <password>命令把普通密码转化为哈希密码并输出。例如：/usr/bin/caddy hash-password --plaintext password


#五、网盘应用5
#.........<省略>........
	@host2 {
		host zz.xx.yy #网盘应用的域名匹配，修改为自己的域名。
	}
	route @host2 {
		root * /home/webdav #修改为自己的服务器存放文件路径
		header {
			Strict-Transport-Security "max-age=31536000; includeSubDomains; preload" #启用HSTS
		}
		@notget {
			not method GET
		}
		route @notget {
			basicauth { #可以设置多组验证用户及密码
				user1 JDJhJDE0JGMzclpVMkU4bVhMRVRXY0o2a2pmMnVGUDNqcGxGZ3ByY1ZTMEVDbi80N1M0NE93NUZXZXNl #webdav用户与webdav密码（示例为password的哈希密码），修改为自己的。
			}
			webdav
		}
		route {
			basicauth { #可以设置多组验证用户及密码
				user2 JDJhJDE0JGMzclpVMkU4bVhMRVRXY0o2a2pmMnVGUDNqcGxGZ3ByY1ZTMEVDbi80N1M0NE93NUZXZXNl #web登录用户与web登录密码（示例为password的哈希密码），修改为自己的。
			}
			file_server browse
		}
	}
#.........<省略>........
#备注：
#1、本配置适合为特定人群提供文件分享及专业的文件管理服务。
#1)、支持web登录（有验证）浏览文件，可下载、音视频文件在线播放等。
#2)、支持webdav客户端（有验证）浏览文件，可删除、上传、修改等。
#3)、web登录与webdav客户端，两者用户及密码各自独立。
#2、本人所有示例的回落或代理网站配置不建议直接替换为上边配置，因为验证的文件服务器做流量伪装与防探测网站有点奇怪。
#1）、建议增加域名来对应增加此配置，实现与原示例的回落或代理网站共存。可参考shunt_Caddyfile，实现逻辑一样。
#2）、web登录方法：域名，如“zz.xx.yy”。
#3、修改/home/webdav的webdav权限为0777，以便客户端具有写入及删除文件权限。
#4、basicauth中密码必须使用哈希密码，可用caddy hash-password --plaintext <password>命令把普通密码转化为哈希密码并输出。例如：/usr/bin/caddy hash-password --plaintext password


#六、网盘应用6
#.........<省略>........
	@host2 {
		host zz.xx.yy #网盘应用的域名匹配，修改为自己的域名。
	}
	route @host2 {
		root * /home/webdav #修改为自己的服务器存放文件路径
		header {
			Strict-Transport-Security "max-age=31536000; includeSubDomains; preload" #启用HSTS
		}
		basicauth { #可以设置多组用户及密码
			user JDJhJDE0JGMzclpVMkU4bVhMRVRXY0o2a2pmMnVGUDNqcGxGZ3ByY1ZTMEVDbi80N1M0NE93NUZXZXNl #用户与密码（示例为password的哈希密码），修改为自己的。
		}
		@notget {
			not method GET
		}
		webdav @notget
		file_server browse
	}
#.........<省略>........
#备注：
#1、本配置适合为个人或特定人群提供专业的文件管理服务。
#1)、支持web登录（有验证）浏览文件，可下载、音视频文件在线播放等。
#2)、支持webdav客户端（有验证）浏览文件，可删除、上传、修改等。
#3)、web登录与webdav客户端，两者用户及密码通用。
#2、本人所有示例的回落或代理网站配置不建议直接替换为上边配置，因为验证的文件服务器做流量伪装与防探测网站有点奇怪。
#1）、建议增加域名来对应增加此配置，实现与原示例的回落或代理网站共存。可参考shunt_Caddyfile，实现逻辑一样。
#2）、web登录方法：域名，如“zz.xx.yy”。
#3、修改/home/webdav的webdav权限为0777，以便客户端具有写入及删除文件权限。
#4、basicauth中密码必须使用哈希密码，可用caddy hash-password --plaintext <password>命令把普通密码转化为哈希密码并输出。例如：/usr/bin/caddy hash-password --plaintext password
